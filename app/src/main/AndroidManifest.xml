<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spiritualtablet">

    <uses-permission-sdk-23 android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission-sdk-23 android:name="android.permission.INTERNET" />
    <uses-permission-sdk-23 android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@drawable/full_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/full_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TeluguBookList"/>
        <activity android:name=".EnglishBookList" />
        <activity android:name=".VideoPlayer" />
        <activity android:name=".Gallery" />
        <activity android:name=".PrimaryCenters" />
        <activity android:name=".ContactUs" />
        <activity android:name=".Organisation" />
        <activity android:name=".Outlets" />
        <activity android:name=".PyramidDoctors" />
        <activity android:name=".Founder" />
        <activity android:name=".Principle" />
        <activity android:name=".History" />
        <activity android:name=".Introduction" />
        <activity android:name=".VideoList" />
        <activity android:name=".About" />
        <activity android:name=".Player" />
        <activity android:name=".AudioActivity" />
        <activity android:name=".ViewBook" />
        <activity android:name=".NoInternet" />
        <activity android:name=".DashBoard" />
        <activity android:name=".SignUp" />
        <activity android:name=".LoggedIn" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".MyMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>