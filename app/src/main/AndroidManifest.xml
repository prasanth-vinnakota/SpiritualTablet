<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spiritualtablet">

    <uses-permission-sdk-23 android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission-sdk-23 android:name="android.permission.INTERNET" />
    <uses-permission-sdk-23 android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/spiritual"
        android:label="@string/app_name"
        android:roundIcon="@drawable/spiritual"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EnglishBooksList"></activity>
        <activity android:name=".ViewBook" />
        <activity android:name=".TeluguBooksList" />
        <activity android:name=".NoInternet" />
        <activity android:name=".VerifyMobileNo" />
        <activity android:name=".DashBoard" />
        <activity android:name=".UserProfile" />
        <activity android:name=".SignUp" />
        <activity android:name=".LoggedIn" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>